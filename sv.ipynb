   "source": [
    "#sv\n",
    "import numpy as np\n",
    "X=np.array([[4,8,2],[4,2,7]])\n",
    "U,S,VT=np.linalg.svd(X)\n",
    "n_comp=2\n",
    "X_recon=np.dot(U[:, :n_comp],np.dot(np.diag(S[:n_comp]),VT[:n_comp, :]))\n",
    "print(\"Original Matrix:\")\n",
    "print(X)\n",
    "print(\"\\nReconstructed Matrix (with reduced dimensions):\")\n",
    "print(X_recon)"
   ]
  },
